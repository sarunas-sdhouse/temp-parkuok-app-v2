{
  "name": "parkuok",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build -p android --profile preview --local && ./upload.sh android",
    "build:ios": "eas build -p ios --profile preview --local && ./upload.sh ios",
    "build:all": "npm run build:android && npm run build:ios",
    "build-dev:android": "eas build -p android --profile development",
    "build-dev:ios": "eas build -p ios --profile development",
    "native:android": "npx expo run:android"
  },
  "dependencies": {
